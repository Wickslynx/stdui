
name: C/C++ CI

on:
  push:
    branches: [ "master" ]
  pull_request:
    branches: [ "master" ]

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v4

    - name: Install library.
      run: ./install.sh

    - name: Run tests.
      run: |
        g++ docs/main.c -o main -lGL -lX11
        ./main



      
